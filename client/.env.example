# Backend API URL (for API calls like /api/auth/login, /api/resume/save, etc.)
VITE_API_URL=http://localhost:5000/api

# Backend Server URL (for OAuth redirects - no /api suffix)
VITE_SERVER_URL=http://localhost:5000

# Adzuna API Credentials (Job Search Feature)
# Get your free API credentials at: https://developer.adzuna.com/
VITE_ADZUNA_APP_ID=your_adzuna_app_id_here
VITE_ADZUNA_APP_KEY=your_adzuna_app_key_here

# Production Example:
# VITE_API_URL=https://your-backend-domain.com/api
# VITE_SERVER_URL=https://your-backend-domain.com
