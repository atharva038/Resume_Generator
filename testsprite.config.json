                "flow": [
                    "Edit summary section",
                    "Add new skill",
                    "Edit experience entry",
                    "Click Save button",
                    "Verify toast notification 'Resume saved successfully'",
                    "Navigate to /dashboard",
                    "Verify resume appears in list"
                ],
                "expectedOutcome": "Resume edited and saved successfully"
            },
            {
                "id": "P0-004",
                "name": "AI Content Enhancement",
                "priority": "P0",
                "prerequisites": [
                    "Resume data loaded in editor"
                ],
                "flow": [
                    "Navigate to editor summary section",
                    "Click 'AI Enhance' button",
                    "Wait for AI processing (2-5 seconds)",
                    "Verify enhanced content displayed",
                    "Verify content improved with action verbs/metrics"
                ],
                "expectedOutcome": "AI enhances content successfully"
            },
            {
                "id": "P0-005",
                "name": "ATS Resume Analysis",
                "priority": "P0",
                "prerequisites": [
                    "User logged in, has saved resume"
                ],
                "flow": [
                    "Navigate to /ats-analyzer",
                    "Select 'ATS Analysis' tab",
                    "Paste job description",
                    "Select resume from dropdown",
                    "Click 'Analyze' button",
                    "Wait for analysis (3-8 seconds)",
                    "Verify ATS score displayed",
                    "Verify keyword matches shown",
                    "Verify suggestions provided"
                ],
                "expectedOutcome": "ATS analysis completed with score and feedback"
            },
            {
                "id": "P1-001",
                "name": "Template Selection",
                "priority": "P1",
                "prerequisites": [
                    "Resume in editor"
                ],
                "flow": [
                    "Open template picker in editor",
                    "Select different template",
                    "Verify preview updates",
                    "Save resume",
                    "Reload resume",
                    "Verify template persisted"
                ],
                "expectedOutcome": "Template changes and persists"
            },
            {
                "id": "P1-002",
                "name": "PDF Export",
                "priority": "P1",
                "prerequisites": [
                    "Resume in editor"
                ],
                "flow": [
                    "Click 'Export PDF' button",
                    "Verify browser print dialog opens",
                    "Check print preview",
                    "Verify resume formatted correctly"
                ],
                "expectedOutcome": "PDF export initiates successfully"
            },
            {
                "id": "P1-003",
                "name": "Dashboard Resume Management",
                "priority": "P1",
                "prerequisites": [
                    "User has multiple saved resumes"
                ],
                "flow": [
                    "Navigate to /dashboard",
                    "Verify resume list displays",
                    "Click resume card to load",
                    "Verify resume opens in editor",
                    "Return to dashboard",
                    "Delete a resume",
                    "Confirm deletion",
                    "Verify resume removed from list"
                ],
                "expectedOutcome": "Dashboard CRUD operations work correctly"
            },
            {
                "id": "P1-004",
                "name": "ML Job Matching",
                "priority": "P1",
                "prerequisites": [
                    "User logged in with resume",
                    "ML service running"
                ],
                "flow": [
                    "Navigate to /ats-analyzer",
                    "Select 'ML Job Matching' tab",
                    "Paste job description",
                    "Select resume",
                    "Click 'Analyze Match'",
                    "Wait for ML processing (2-4 seconds)",
                    "Verify match score displayed",
                    "Verify skill gap analysis shown",
                    "Verify recommendations provided"
                ],
                "expectedOutcome": "ML analysis provides match score and gaps"
            },
            {
                "id": "P2-001",
                "name": "GitHub Project Import",
                "priority": "P2",
                "prerequisites": [
                    "User logged in",
                    "GitHub OAuth configured"
                ],
                "flow": [
                    "Navigate to /github-import",
                    "Click 'Connect GitHub'",
                    "Complete OAuth flow",
                    "Verify repository list loads",
                    "Select repositories",
                    "Click 'Import Selected'",
                    "Navigate to /editor",
                    "Verify projects imported"
                ],
                "expectedOutcome": "GitHub projects imported successfully"
            }
        ]
    },
