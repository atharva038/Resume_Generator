    "testScenarios": {
        "priority": "high",
        "scenarios": [
            {
                "id": "P0-001",
                "name": "User Registration & Login",
                "priority": "P0",
                "flow": [
                    "Navigate to /register",
                    "Fill registration form with valid data",
                    "Submit form",
                    "Verify redirect to /dashboard",
                    "Verify user is logged in",
                    "Logout",
                    "Login with same credentials",
                    "Verify successful login"
                ],
                "expectedOutcome": "User can register, login, and access dashboard"
            },
            {
                "id": "P0-002",
                "name": "Resume Upload & Parse",
                "priority": "P0",
                "prerequisites": [
                    "User must be logged in"
                ],
                "flow": [
                    "Navigate to /upload",
                    "Upload sample-resume.pdf",
                    "Wait for AI parsing (3-10 seconds)",
                    "Verify redirect to /editor",
                    "Verify parsed data displayed correctly"
