{
    "projectName": "SmartNShine ATS Resume Builder",
    "version": "1.0.0",
    "description": "AI-powered ATS Resume Builder with MERN Stack + Gemini AI",
    "testConfiguration": {
        "frontend": {
            "type": "frontend",
            "localPort": 5173,
            "pathname": "/",
            "testScope": "codebase",
            "framework": "React + Vite",
            "needLogin": true,
            "projectPath": "/Users/atharva_beast/Desktop/Coading/Full-Stack-Web/ATS_RESUME_GENERATOR"
        },
        "backend": {
            "type": "backend",
            "localPort": 5000,
            "baseURL": "http://localhost:5000/api",
