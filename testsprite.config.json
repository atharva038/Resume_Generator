            "needAuth": true
        }
    },
    "prerequisites": {
        "services": [
            {
                "name": "MongoDB",
                "required": true,
                "defaultURL": "mongodb://localhost:27017/smartnshine"
            },
            {
                "name": "Backend Server",
                "required": true,
                "port": 5000,
                "startCommand": "cd server && npm run dev"
            },
            {
                "name": "Frontend Dev Server",
                "required": true,
                "port": 5173,
                "startCommand": "cd client && npm run dev"
            },
            {
                "name": "ML Service (Optional)",
                "required": false,
                "port": 8000,
                "startCommand": "cd ml-service && python app.py"
            }
        ],
        "environmentVariables": {
            "backend": [
